<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Makes HTTP requests easier</Description>
    <PackageTags>http rest api httpwrapper</PackageTags>
    <PackageLicenseUrl>https://github.com/pimbrouwers/Cinch.HTTipi/blob/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>http://www.pimbrouwers.com/cinch.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/pimbrouwers/Cinch.HTTipi</PackageProjectUrl>
    <Copyright>2017</Copyright>
    <RepositoryUrl>https://github.com/pimbrouwers/Cinch.HTTipi</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <FileVersion>1.0.0.8</FileVersion>
    <Version>1.0.8</Version>
    <AssemblyVersion>1.0.0.8</AssemblyVersion>
    <PackageReleaseNotes>responseMessageHandler Action&lt;HttpResponseMessage&gt; added to all methods. Allows consumer to interact directly with response.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
